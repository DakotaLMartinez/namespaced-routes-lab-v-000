<h1>Admin::Access#index</h1>
<%= form_for @preference, url: admin_access_path(@preference) do |f| %>
    <p>Allow Creating Songs: True <%= f.radio_button :allow_create_songs, true %> False <%= f.radio_button :allow_create_songs, false %></p>
    <p>Allow Creating Artists: True <%= f.radio_button :allow_create_artists, true %> False <%= f.radio_button :allow_create_artists, false %></p>
<%= f.submit %>
<% end %>