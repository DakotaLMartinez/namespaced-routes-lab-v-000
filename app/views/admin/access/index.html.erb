<h1>Admin::Access#index</h1>
<%= form_for @preference, url: admin_access_path(@preference) do |f| %>
    <p><%= f.label :allow_create_songs %>: True <%= f.radio_button :allow_create_songs, true %> False <%= f.radio_button :allow_create_songs, false %></p>
    <p><%= f.label :allow_create_artists %>: True <%= f.radio_button :allow_create_artists, true %> False <%= f.radio_button :allow_create_artists, false %></p>
<%= f.submit %>
<% end %>